jenkins:
  agentProtocols:
  - "JNLP4-connect"
  - "Ping"
  crumbIssuer:
    standard:
      excludeClientIPFromCrumb: false
  remotingSecurity:
    enabled: true
  slaveAgentPort: 50000
plugins:
  sites:
  - id: "default"
    url: "https://updates.jenkins.io/update-center.json"
security:
  remotingCLI:
    enabled: false
unclassified:
  location:
    url: "http://<%= spec.ip %>:<%= p('jenkins.config.http_port') %>"
tool:
  git:
    installations:
    - name: "git"
      home: "/var/vcap/packages/git-client/bin"
  jdk:
    installations:
    - name: "jdk"
      home: "/var/vcap/packages/oracle-jdk"
