jenkins:
  agentProtocols:
  - "JNLP4-connect"
  - "Ping"
  crumbIssuer:
    standard:
      excludeClientIPFromCrumb: false
  labelString: "<%= p('labels', '') %>"
  mode: <%= p('mode').upcase %>
  numExecutors: <%= p('executors') %>
  remotingSecurity:
    enabled: true
  slaveAgentPort: 50000
plugins:
  sites:
  - id: "default"
    url: "https://updates.jenkins.io/update-center.json"
security:
  remotingCLI:
    enabled: false
unclassified:
  location:
    url: "http://<%= spec.ip %>:<%= p('port') %>"
tool:
  git:
    installations:
    - name: "Default"
      home: "/var/vcap/packages/git-client/bin/git"
  jdk:
    installations:
    - name: "Default"
      home: "/var/vcap/packages/oracle-jdk"
